<div class="member-item">
  <header class="pageheader">
    <h1>Create Member</h1>
  </header>
  <%= form_for @member, html: { multipart: true, class: "form" } do |f| %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div>
      <%= f.label :email %>
      <%= f.email_field :email %>
    </div>
    <div>
      <%= f.label :phone %>
      <%= f.telephone_field :phone %>
    </div>
    <div>
      <%= f.label :address %>
      <%= f.text_field :address %>
    </div>
    <div>
      <%= f.label :bio %>
      <%= f.text_area :bio, rows: "10" %>
    </div>
    <div>
      <%= f.label :bibliography %>
      <%= f.url_field :bibliography %>
    </div>
    <div>
      <%= f.label :member_pic %>
      <%= f.file_field :member_pic %>
    </div>
    <div>
      <%= f.submit %>
    </div>
  <% end %>
</div>
